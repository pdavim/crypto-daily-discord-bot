{
  "source": {
    "include": ["src"],
    "includePattern": ".*\\.js$",
    "excludePattern": "(^|/)tests/"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "light",
      "title": "Crypto Daily Discord Bot API",
      "homepageTitle": "Crypto Daily Discord Bot API"
    }
  },
  "plugins": []
}
